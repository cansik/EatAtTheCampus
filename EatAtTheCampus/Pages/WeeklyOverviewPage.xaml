<?xml version="1.0" encoding="UTF-8"?>
<CarouselPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="EatAtTheCampus.WeeklyOverviewPage" xmlns:local="clr-namespace:EatAtTheCampus;assembly=EatAtTheCampus">
	<ContentPage x:Name="defaultPage">
		<ContentPage.Content>
			<AbsoluteLayout>
				<!-- background image -->
				<!--<Image Source="{local:ImageResource EatAtTheCampus.Resources.background.jpg}" Aspect="AspectFill" AbsoluteLayout.LayoutBounds="0, 0, 1, 1" AbsoluteLayout.LayoutFlags="All" />
				-->
				<ScrollView AbsoluteLayout.LayoutBounds="0.2, 0.2, 0.8, 0.8" Padding="20, 0, 20, 0" AbsoluteLayout.LayoutFlags="All" VerticalOptions="FillAndExpand">
					<StackLayout x:Name="resultLayout" Padding="10">
						<ActivityIndicator x:Name="activityIndicator" Color="Gray" IsRunning="true" />
					</StackLayout>
				</ScrollView>
			</AbsoluteLayout>
		</ContentPage.Content>
	</ContentPage>
</CarouselPage>